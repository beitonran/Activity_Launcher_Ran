<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">



    <!-- Override Styles -->
    <Style x:Key="GroupBoxStyleOverride" TargetType="{x:Type GroupBox}" BasedOn="{StaticResource GroupBoxStyle}">
        <Setter Property="BorderBrush" Value="#FF939090" />
        <Setter Property="Foreground" Value="#FF939090" />
    </Style>

    <Style x:Key="MaterialDesignFloatingHintTextBox" BasedOn="{StaticResource MaterialDesignFloatingHintTextBox}" TargetType="{x:Type TextBox}">
        <Setter Property="FontSize" Value="24" />
        <Setter Property="materialDesign:HintAssist.FloatingScale" Value="0.6" />
        <Setter Property="materialDesign:HintAssist.FloatingOffset" Value="1,-20" />
    </Style>

    <Style x:Key="HeaderStyleOverride" TargetType="{x:Type TextBlock}">
        <Setter Property="FontSize" Value="12" />
    </Style>

  
</ResourceDictionary>



